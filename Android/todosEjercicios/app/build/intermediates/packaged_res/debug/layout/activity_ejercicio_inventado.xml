<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ut03.EjercicioInventado">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentPerfil2"
        android:layout_width="410dp"
        android:layout_height="291dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentPerfil1"
        android:name="com.example.todosejercicios.ut03.EjercicioInventadoFragment"
        android:layout_width="415dp"
        android:layout_height="281dp"
        app:layout_constraintBottom_toTopOf="@+id/fragmentPerfil2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvEdadTotal"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/fragmentPerfil1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.236" />

    <TextView
        android:id="@+id/tvSeekBarProgress"
        android:layout_width="239dp"
        android:layout_height="46dp"
        app:layout_constraintBottom_toTopOf="@+id/fragmentPerfil1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvEdadTotal" />
</androidx.constraintlayout.widget.ConstraintLayout>